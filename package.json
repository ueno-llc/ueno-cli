{
	"name": "ueno-cli",
	"version": "0.1.9",
	"description": "Ueno Command-Line Utility",
	"license": "MIT",
	"repository": "ueno-llc/ueno-cli",
	"author": {
		"name": "Birkir Gudjonsson",
		"email": "birkir.gudjonsson@gmail.com"
	},
	"bin": "build/src/index.js",
	"engines": {
		"node": ">=6"
	},
	"scripts": {
		"prepare": "npm run build",
		"clean": "rimraf ./build",
		"lint": "tslint -p tsconfig.json",
		"build": "npm run clean && tsc -p tsconfig.prod.json",
		"test": "npm run build && npm run lint && jest"
	},
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
	"files": [
		"build/*"
	],
	"keywords": [
		"ueno",
		"react",
		"react-native"
	],
	"dependencies": {
		"ink-spinner": "2.0.0",
		"ink": "0.5.1",
		"tslib": "1.9.3",
		"yargs": "12.0.1"
	},
	"devDependencies": {
		"@babel/core": "7.0.0-beta.56",
		"@types/jest": "23.3.1",
		"@types/node": "10.5.7",
		"@types/react": "16.4.8",
		"@types/react-dom": "16.0.7",
		"@types/yargs": "11.1.1",
		"babel-jest": "23.4.2",
		"husky": "1.0.0-rc.13",
		"jest": "23.4.2",
		"rimraf": "2.6.2",
		"ts-jest": "23.1.3",
		"tslint": "5.11.0",
		"tslint-microsoft-contrib": "5.2.0",
		"typescript": "3.0.1"
	},
	"jest": {
		"testEnvironment": "node",
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		],
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts|js)x?$"
	}
}
